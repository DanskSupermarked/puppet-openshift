apiVersion: v1
clusters:
- cluster:
    certificate-authority-data:
    server: <%= @openshift::master_url_internal %>
  name: <%= @openshift::cluster_name %>
contexts:
- context:
    cluster: <%= @openshift::cluster_name %>
    namespace: default
    user: system:node:<%= @node_fqdn_internal %>/<%= @openshift::cluster_name %>
  name: default/<%= @openshift::cluster_name %>/system:node:<%= @openshift::node_fqdn_internal %>
current-context:
kind: Config
preferences: {}
users:
- name: system:node:<%= @openshift::node_fqdn_internal %>/<%= @openshift::cluster_name %>
  user:
    client-certificate-data:
    client-key-data:
