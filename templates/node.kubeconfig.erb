apiVersion: v1
clusters:
- cluster:
    certificate-authority-data:
    server: <%= scope['openshift::master_url_internal'] %>
  name: <%= scope['openshift::cluster_name'] %>
contexts:
- context:
    cluster: <%= scope['openshift::cluster_name'] %>
    namespace: default
    user: system:node:<%= scope['node_fqdn_internal'] %>/<%= scope['openshift::cluster_name'] %>
  name: default/<%= scope['openshift::cluster_name'] %>/system:node:<%= scope['openshift::node_fqdn_internal'] %>
current-context:
kind: Config
preferences: {}
users:
- name: system:node:<%= scope['openshift::node_fqdn_internal'] %>/<%= scope['openshift::cluster_name'] %>
  user:
    client-certificate-data:
    client-key-data:
